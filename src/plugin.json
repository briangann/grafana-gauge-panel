{
    "$schema": "https://github.com/grafana/grafana/raw/master/docs/sources/developers/plugins/plugin.schema.json",
    "type": "panel",
    "name": "D3 Gauge",
    "id": "briangann-gauge-panel",
    "info": {
        "description": "D3-based Gauge panel for Grafana",
        "author": {
            "name": "Brian Gann (bkgann@gmail.com)",
            "url": "https://github.com/briangann"
        },
        "keywords": ["gauge", "panel"],
        "logos": {
            "small": "img/Logo_D3.svg",
            "large": "img/Logo_D3.svg"
        },
        "links": [{
            "name": "Project site",
            "url": "https://github.com/briangann/grafana-gauge-panel"
        }, {
            "name": "MIT License",
            "url": "https://github.com/briangann/grafana-gauge-panel/blob/main/LICENSE"
        }],
        "screenshots": [{
            "name": "Showcase",
            "path": "screenshots/default-gauge-w-threshold.png"
        }, {
            "name": "Options",
            "path": "screenshots/options.png"
        }, {
            "name": "Radial Metrics",
            "path": "screenshots/radialmetrics.png"
        }, {
            "name": "Thresholding",
            "path": "screenshots/thresholding.png"
        }],
        "version": "%VERSION%",
        "updated": "%TODAY%"
    },

    "dependencies": {
        "grafanaDependency": ">=6.3.0",
        "grafanaVersion": "6.x.x",
        "plugins": []
    }
