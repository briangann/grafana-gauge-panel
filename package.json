{
  "name": "briangann-gauge-panel",
  "private": true,
  "version": "1.0.0",
  "description": "Grafana D3 Gauge Panel",
  "main": "module.js",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "server": "docker-compose up --build",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briangann/grafana-gauge-panel.git"
  },
  "author": "Brian Gann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/briangann/grafana-gauge-panel/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@grafana/e2e": "9.3.8",
    "@grafana/e2e-selectors": "9.3.8",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.2.144",
    "@swc/helpers": "^0.4.12",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/d3": "^5.16.3",
    "@types/d3-scale": "3.2.2",
    "@types/glob": "^8.0.0",
    "@types/grafana": "github:CorpGlory/types-grafana.git",
    "@types/jest": "^29.2.2",
    "@types/jquery": "3.5.5",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "d3": "5.16.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.0",
    "glob": "^8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.3.1",
    "jest-junit": "^12.0.0",
    "prettier": "^2.5.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.56.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.4.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2",
    "jest-environment-jsdom": "^29.3.1"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "9.3.8",
    "@grafana/runtime": "9.3.8",
    "@grafana/ui": "9.3.8",
    "d3-ease": "^1.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "resolutions": {
    "rxjs": "6.6.3"
  },
  "homepage": "https://github.com/briangann/grafana-gauge-panel",
  "engines": {
    "node": ">= 16"
  },
  "packageManager": "yarn@1.22.19"
}
