{
  "name": "briangann-gauge-panel",
  "private": true,
  "version": "2.0.1",
  "description": "Grafana D3 Gauge Panel",
  "main": "module.js",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "pnpm run lint --fix",
    "e2e": "pnpm exec cypress install && pnpm exec grafana-e2e run",
    "e2e:update": "pnpm exec cypress install && pnpm exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briangann/grafana-gauge-panel.git"
  },
  "author": "Brian Gann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/briangann/grafana-gauge-panel/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "10.1.5",
    "@grafana/e2e-selectors": "10.1.5",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/d3": "7.4.2",
    "@types/d3-scale": "4.0.6",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "18.18.2",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "7.3.8",
    "css-loader": "^6.7.3",
    "d3": "7.8.5",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "9.3.16",
    "@grafana/runtime": "9.3.16",
    "@grafana/ui": "9.3.16",
    "@types/uuid": "8.3.4",
    "d3-ease": "3.0.1",
    "d3-scale": "4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semver": "^7.5.4",
    "tslib": "2.5.3",
    "uuid": "^9.0.1"
  },
  "homepage": "https://github.com/briangann/grafana-gauge-panel",
  "engines": {
    "node": ">= 18"
  },
  "packageManager": "pnpm@8.9.2"
}
